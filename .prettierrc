{
  "$schema": "https://json.schemastore.org/prettierrc",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "none",
  "printWidth": 120,
  "proseWrap": "always",
  "useTabs": false,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "singleAttributePerLine": true,
  "jsxSingleQuote": false,
  "quoteProps": "consistent",
  "endOfLine": "lf",
  "plugins": [
    "prettier-plugin-organize-imports"
  ]
}